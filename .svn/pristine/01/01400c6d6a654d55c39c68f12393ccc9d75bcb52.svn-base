package datos;

public class FrigobarItem {
	private int idFrigobarItem;
	private int stockExistente;
	private int stockDeseable;
	private Consumo consumo;
	private Frigobar frigobar;

	public FrigobarItem(){
		
	}
	public FrigobarItem( int stockExistente, int stockDeseable,Consumo consumo,Frigobar frigobar) {
		this.stockExistente = stockExistente;
		this.stockDeseable = stockDeseable;
		this.consumo = consumo;
		this.frigobar = frigobar;
	}
	

	public int getIdFrigobarItem() {
		return idFrigobarItem;
	}
	public void setIdFrigobarItem(int idFrigobarItem) {
		this.idFrigobarItem = idFrigobarItem;
	}
	public int getStockExistente() {
		return stockExistente;
	}
	public void setStockExistente(int stockExistente) {
		this.stockExistente = stockExistente;
	}
	public int getStockDeseable() {
		return stockDeseable;
	}
	public void setStockDeseable(int stockDeseable) {
		this.stockDeseable = stockDeseable;
	}
	public Consumo getConsumo() {
		return consumo;
	}
	public void setConsumo(Consumo consumo) {
		this.consumo = consumo;
	}
	public Frigobar getFrigobar() {
		return frigobar;
	}
	public void setFrigobar(Frigobar frigobar) {
		this.frigobar = frigobar;
	}
	@Override
	public String toString() {
		return "Frigobar [idFrigobar=" + idFrigobarItem + ", stockExistente="
				+ stockExistente + ", stockDeseable=" + stockDeseable
				+  "]";
	}

}
