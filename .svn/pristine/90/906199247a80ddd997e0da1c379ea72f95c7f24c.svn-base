package datos;

import java.util.Set;

public class TipoMantenimiento {
	
	private int idTipoMantenimiento;
	private String tipoMantenimiento;
	private Set<TicketMantenimiento> ticketMantenimiento;

	public TipoMantenimiento() {
	}

	public TipoMantenimiento(String tipoMantenimiento) {
		this.tipoMantenimiento = tipoMantenimiento;
	}

	public Set<TicketMantenimiento> getTicketMantenimiento() {
		return ticketMantenimiento;
	}

	public void setTicketMantenimiento(Set<TicketMantenimiento> ticketMantenimiento) {
		this.ticketMantenimiento = ticketMantenimiento;
	}

	public int getIdTipoMantenimiento() {
		return idTipoMantenimiento;
	}

	protected void setIdTipoMantenimiento(int idTipoMantenimiento) {
		this.idTipoMantenimiento = idTipoMantenimiento;
	}

	public String getTipoMantenimiento() {
		return tipoMantenimiento;
	}

	public void setTipoMantenimiento(String tipoMantenimiento) {
		this.tipoMantenimiento = tipoMantenimiento;
	}

	@Override
	public String toString() {
		return "TipoMantenimiento [idTipoMantenimiento=" + idTipoMantenimiento + ", tipoMantenimiento="
				+ tipoMantenimiento + "]";
	}

}
