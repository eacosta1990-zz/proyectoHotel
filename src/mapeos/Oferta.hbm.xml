<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="datos.Oferta" table="oferta">
		<id column="idOferta" name="idOferta">
			<generator class="identity" />
		</id>
		<property name="descuento" type="float" column="descuento" />
		<property name="fechaDesde" type="calendar" column="fechaDesde" />
		<property name="fechaHasta" type="calendar" column="fechaHasta" />
		<property name="estado" type="boolean" column="estado" />
		
	</class>
</hibernate-mapping>
